	pages = {8706--8715},
	file = {PubMed Central Full Text PDF:/Users/chengk6/Zotero/storage/IYUJQVCS/Carretero-González et al. - 2018 - Analysis of response rate with ANTI PD1PD-L1 mono.pdf:application/pdf}
}

