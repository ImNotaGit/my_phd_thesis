	pmid = {33264556},
	pmcid = {PMC7727327},
	file = {Full Text:/Users/chengk6/Zotero/storage/TZ9ARNI8/WHO Solidarity Trial Consortium et al. - 2020 - Repurposed Antiviral Drugs for Covid-19 - Interim .pdf:application/pdf}
