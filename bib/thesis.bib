@article{scharping_tumor_2016,
	title = {The {Tumor} {Microenvironment} {Represses} {T} {Cell} {Mitochondrial} {Biogenesis} to {Drive} {Intratumoral} {T} {Cell} {Metabolic} {Insufficiency} and {Dysfunction}},
