	month = mar,
	year = {2020},
	note = {Publisher: Sciendo
Section: Acta Haematologica Polonica},
	pages = {2--5},
	file = {Full Text PDF:/Users/chengk6/Zotero/storage/XXAPZRXM/Styczyński - 2020 - A brief history of CAR-T cells from laboratory to.pdf:application/pdf;Snapshot:/Users/chengk6/Zotero/storage/EGUQ7WPB/journals\$002fahp\$002f51\$002f1\$002farticle-p2.html:text/html}
