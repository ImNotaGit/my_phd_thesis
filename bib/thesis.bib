}

@article{deng_antitumor_2015,
	title = {Antitumor immunity. {A} shed {NKG2D} ligand that promotes natural killer cell activation and tumor rejection},
	volume = {348},
	issn = {1095-9203},
	doi = {10.1126/science.1258867},
