	number = {16},
	journal = {N Engl J Med},
	author = {Grupp, Stephan A. and Kalos, Michael and Barrett, David and Aplenc, Richard and Porter, David L. and Rheingold, Susan R. and Teachey, David T. and Chew, Anne and Hauck, Bernd and Wright, J. Fraser and Milone, Michael C. and Levine, Bruce L. and June, Carl H.},
