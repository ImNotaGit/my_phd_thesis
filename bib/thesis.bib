	keywords = {Algorithms, Computational Biology, Models, Biological, Software, Gene Expression Regulation, Gene Expression Profiling, Models, Statistical, Reproducibility of Results, Genome, Bacterial, Cluster Analysis, Mycobacterium tuberculosis, Metabolic Networks and Pathways, Fatty Acids, Gene Expression Regulation, Bacterial, Mycolic Acids},
	pages = {e1000489},
