	pmcid = {PMC6357182},
	keywords = {Humans, Chromatography, Liquid, HCoV-229E, lipidomics, MERS-CoV, UHPLC–MS, Cell Line, Virus Replication, Host-Pathogen Interactions, Arachidonic Acid, Coronavirus 229E, Human, Glycerophospholipids, Linoleic Acid, Lipid Metabolism, Tandem Mass Spectrometry},
