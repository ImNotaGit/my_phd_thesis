@article{konno_sars-cov-2_2020,
	title = {{SARS}-{CoV}-2 {ORF3b} {Is} a {Potent} {Interferon} {Antagonist} {Whose} {Activity} {Is} {Increased} by a {Naturally} {Occurring} {Elongation} {Variant}},
