	keywords = {Female, Humans, Male, Adult, Middle Aged, Young Adult, Aged, Comorbidity, COVID-19, Betacoronavirus, Coronavirus Infections, Pneumonia, Viral, SARS-CoV-2, Pandemics, Age Factors, Hospitalization, Risk Factors, Critical Illness, Respiration, Artificial, Critical Care, New York City, Prospective Studies},
	pages = {m1966},
	file = {Full Text:/Users/chengk6/Zotero/storage/3MYNYH5M/Petrilli et al. - 2020 - Factors associated with hospital admission and cri.pdf:application/pdf}
}

