	keywords = {Female, Humans, Male, Adult, Middle Aged, Young Adult, Aged, Aged, 80 and over, COVID-19, Betacoronavirus, China, Coronavirus Infections, Pneumonia, Viral, SARS-CoV-2, Pandemics, Hospitalization, Cohort Studies, Heart Diseases, Hospital Mortality, Survival Rate},
	pages = {802--810},
