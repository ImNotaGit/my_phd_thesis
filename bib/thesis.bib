	pages = {e1000082},
	file = {Full Text:/Users/chengk6/Zotero/storage/SFPDHE95/Becker and Palsson - 2008 - Context-specific metabolic networks are consistent.pdf:application/pdf}
}

@article{schmidt_gim3e_2013,
	title = {{GIM3E}: condition-specific models of cellular metabolism developed from metabolomics and expression data},
