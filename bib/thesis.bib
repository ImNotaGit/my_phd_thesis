	keywords = {Female, Humans, Male, Adolescent, Adult, Child, Middle Aged, Young Adult, Aged, Aged, 80 and over, Polymerase Chain Reaction, Comorbidity, COVID-19, SARS-CoV-2, COVID-19 Testing, Family Characteristics, Germany, Immunoglobulin G, Mortality, Prevalence},
