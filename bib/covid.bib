	keywords = {Humans, Animals, Protein Biosynthesis, Ribosomes, Gene Expression Profiling, Molecular Sequence Annotation, Open Reading Frames, Genome, Viral, SARS-CoV-2, RNA, Viral, Cell Line, Viral Proteins, Peptides, RNA, Messenger},
	pages = {125--130},
