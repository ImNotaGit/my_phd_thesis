	keywords = {Humans, COVID-19, Angiotensin-Converting Enzyme 2, Risk Factors, Blood Glucose, Diabetes Mellitus, Type 1, Diabetes Mellitus, Type 2, Disease Management, Hypoglycemic Agents},
	pages = {11--30},
	file = {Full Text:/Users/chengk6/Zotero/storage/GHFI6KE3/Lim et al. - 2021 - COVID-19 and diabetes mellitus from pathophysiolo.pdf:application/pdf}
