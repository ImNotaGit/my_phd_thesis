	urldate = {2021-02-09},
	institution = {Social Science Research Network},
	author = {Ehrlich, Avner and Uhl, Skyler and Ioannidis, Konstantinos and Hofree, Matan and tenOever, Benjamin R. and Nahmias, Yaakov},
	month = jul,
	year = {2020},
	doi = {10.2139/ssrn.3650499},
