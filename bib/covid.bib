	author = {dos Santos, Wagner Gouvea},
	month = sep,
	year = {2020},
	keywords = {SARS-CoV-2, Anakinra, Convalescent plasma, Corticosteroids, Hydroxychloroquine, Vaccine},
	pages = {110493}
}

